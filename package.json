    "vuejs",
    "vite",
    "guoguo-ui",
    "ui",
    "vueui",
    "vertical",
    "horizontal",
    "components",
    "vue组件库",
    "vue3组件库"
  ],
  "files": [
    "dist/*"
  ],
  "license": "GuoGuo",
  "homepage": "https://shixindea.github.io/#/mzlui",
  "bugs": {
    "url": "https://shixindea.github.io/#/mzlui",
    "email": "2914310412@qq.com"
  },
  "scripts": {
    "start": "vite --port 3002",
    "dev": "vite --port 3002",
    "build": "vite build",
    "build:html": "vite build --config ./build/build.js",
    "build:doc": "vite build --config ./build/build.js",
    "preview": "vite preview",
    "guoguo-ui": "vite build",
    "test": "jest --runInBand",
    "gitcz": "git-cz",
    "deploy": "gh-pages -d dist"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vue/reactivity": "^3.2.31",
    "@vue/test-utils": "^2.0.0-rc.18",
    "babel-jest": "26.*",
    "babel-preset-vite": "^1.0.4",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.13.0",
    "eslint-plugin-vue": "^8.6.0",
    "highlight.js": "^11.4.0",
    "jest": "26.6.3",
    "pinia": "^2.0.36",
    "sass": "^1.49.7",
    "sass-loader": "^12.6.0",
    "vite": "^2.7.2",
    "vite-plugin-md": "^0.12.4",
    "vue": "^3.2.25",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-router": "^4.0.14"
  },
  "engines": {
    "node": ">= 14"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
